public class palindromeArray
{
    public static void main(String[] args) {
        int arr[]=new int { 1,2,3,2,1};
        palindromeArray(arr);
    }
    public static void palindromeArray(int arr[])
    {
        for(int i=0;i<arr.length/2;i++)
        {
